{
  "name": "njwt",
  "version": "1.1.0",
  "description": "JWT Library for Node.js",
  "main": "index.js",
  "scripts": {
    "test": "npx istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly test/ -- -R spec --no-timeouts;",
    "test-watch": "npx mocha --timeout=5000 --reporter dot --check-leaks -w ./*.js test/ ",
    "test-debug": "npx mocha --timeout=5000 --debug --reporter dot --check-leaks -w ./*.js test/ ",
    "test-cov": "npx istanbul cover node_modules/mocha/bin/_mocha -- --timeout=5000 --reporter dot --check-leaks test/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sambacha/njwt"
  },
  "keywords": [
    "jwt"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sambacha/njwt/issues"
  },
  "homepage": "https://github.com/sambacha/njwt",
  "dependencies": {
    "ecdsa-sig-formatter": "1.0.11",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.5.0",
    "jwt-simple": "0.5.6",
    "mocha": "^6.0.2",
    "secure-random": "1.1.2"
  }
}
